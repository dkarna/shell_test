#! /bin/bash

Unix=('1' '4' '3' '2' '6');
echo "count: `expr ${#Unix} + 1`"
cnt=${#Unix}

echo "Before sorting: "

for((i=0; i <= cnt; i++)); do
	echo ${Unix[i]}
done

for((i=0; i <= cnt; i++)); do
	for((j=$i; j <= cnt; j++)); do
		if  [ "${Unix[$i]}" -gt "${Unix[$j]}" ] 
		then
			echo "inside"
			temp=${Unix[$i]}
			${Unix[$i]}=${Unix[$j]}
			${Unix[$j]}=$temp
		else
			echo "outside"
		fi
	done
done

echo "After sorting: "

for((i=0; i <= cnt; i++)); do
	echo ${Unix[i]}
done

echo "Byee.."
